{
  "name": "@provenance/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for PIC Trust Plane - Provenance Identity Continuity",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./middleware": {
      "types": "./dist/middleware/index.d.ts",
      "import": "./dist/middleware/index.mjs",
      "require": "./dist/middleware/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "provenance",
    "trust",
    "pic",
    "authority",
    "security",
    "confused-deputy"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/clay-good/provenance.git",
    "directory": "sdks/typescript"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@noble/ed25519": "^2.1.0",
    "cborg": "^4.2.0"
  },
  "peerDependencies": {
    "express": "^4.18.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "express": "^4.18.2",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^4.0.18"
  }
}
